{
    "builds":[{
        "src": "word_learner_api/word_learner_api/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "word_learner_api/word_learner_api/wsgi.py"
        }
    ]
}


